{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "spotipix",
  "version": "0.1.0",
  "identifier": "com.ananya.spotipix",
  "build": {
    "frontendDist": "../src"
  },
  "allowlist": {
    "shell": {
      "open": true
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "spotipix",
        "url": "index.html",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "identifier": "com.ananya.spotipix",
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "env": {
  "SPOTIFY_CLIENT_ID": { "type": "string", "value": "9d7d25f6b40146db8a5a78b054fd34fa" },
  "SPOTIFY_CLIENT_SECRET": { "type": "string", "value": "eb92c935ab194d5894bd68af09e98bf8" },
  "REDIRECT_URI": { "type": "string", "value": "http://localhost:1420" }
},
"protocol": {
    "custom": {
      "scheme": "spotipix",
      "handler": "callback"
    }
}
}
